{
    "version": 2,
    "trailingSlash": false,
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node",
            "config": { "outputDirectory": "dist" }
        }
    ],
    "routes": [
        {
            "handle": "cors"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/main.js"
        }
    ]
}
